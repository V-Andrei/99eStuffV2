

@{
    ViewBag.Title = "Contact";
    if (Session["userID"] == null)
    {
        Response.Redirect("~/Login/LoginRegister");
    }
}
<!doctype html>
<html class="no-js" lang="zxx">
<head>
    <title>LogIn Ragister || 99eStuff</title>
</head>

<body>
    <!-- Start Login Register Area -->
    <div class="htc__login__register bg__white ptb--130" style="background: rgba(0, 0, 0, 0) url(../../Content/img/login/loginBg.jpg) no-repeat scroll center center;">
        <div class="container">
            <div class="row">
                <div class="col-md-6 ml-auto mr-auto">
                    <ul class="login__register__menu nav justify-contend-center" role="tablist">
                        <li role="presentation" class="login active"><a class="active" href="#login" role="tab" data-toggle="tab">Dashboard</a></li>
                    </ul>
                </div>
            </div>
            <!-- Start Login Register Content -->
            <div class="row tab-content">
                <div class="col-md-6  ml-auto mr-auto">
                    <div class="htc__login__register__wrap">
                        <!-- Start Single Content -->
                        <div id="login" role="tabpanel" class="single__tabs__panel tab-pane active">
                            <div>
                                <h1>Username : @Session["userName"].ToString()</h1>
                                <h1>Mail: @Session["mail"]</h1>
                                <h2>Name: @Session["nameClient"]</h2>
                            </div>                   
                            <div class="htc__login__btn mt--30">
                                <a href="@Url.Action("LogOut","Login")">LogOut</a>
                            </div>                           
                        </div>
                        <!-- End Single Content -->
                    </div>
                </div>
            </div>
            <!-- End Login Register Content -->
        </div>
    </div>
</body>

</html>